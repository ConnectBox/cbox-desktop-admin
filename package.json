{
  "name": "cbox-desktop-admin-app",
  "productName": "ConnectBox Desktop Admin",
  "description": "Admin Application for configuring a ConnectBox",
  "version": "0.2.03",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "axios": "^0.17.0",
    "bluebird": "^3.5.1",
    "create-react-class": "^15.6.3",
    "drivelist": "^6.1.7",
    "electron": "^1.7.9",
    "electron-download-manager": "^1.3.1",
    "electron-platform": "^1.2.0",
    "fs-extra": "^4.0.2",
    "immutability-helper": "^2.6.0",
    "localforage": "^1.6.0",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-data-grid": "^3.0.11",
    "react-dom": "^16.0.0",
    "react-photo-gallery": "^6.0.15",
    "react-player": "^1.2.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-tap-event-plugin": "^3.0.2",
    "react-tappable": "^1.0.2",
    "shorthash": "^0.0.2",
    "soundmanager2": "^2.97.20170602",
    "typeface-roboto": "^0.0.43",
    "unzipper": "^0.8.14",
    "url": "^0.11.0",
    "verge": "^1.10.2"
  },
  "devDependencies": {
    "electron-rebuild": "^1.7.3",
    "electron-winstaller": "^2.6.4",
    "foreman": "^2.0.0",
    "react-scripts": "1.0.14"
  },
  "homepage": "./",
  "main": "electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "package-mac": "electron-packager . --overwrite --platform=darwin --ignore=cred.js --arch=x64 --icon=./icons/mac/CBox.ico --prune=true --out=release-builds",
    "package-win32": "electron-packager . cbox-desktop-admin-app --overwrite --platform=win32 --ignore=cred.js --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=Qombi --version-string.FileDescription=V2.02 --version-string.ProductName=\"CBox Desktop Admin App\"",
    "package-win": "electron-packager . cbox-desktop-admin-app --overwrite --platform=win32 --ignore=cred.js --arch=x64 --icon=./icons/win/CBox.ico --prune=true --out=release-builds --version-string.CompanyName=Qombi --version-string.FileDescription=V2.02 --version-string.ProductName=\"CBox Desktop Admin App\"",
    "package-linux": "electron-packager . cbox-desktop-admin-app --overwrite --asar --platform=linux --ignore=cred.js --arch=x64 --icon=icons/linux/CBox.png --prune=true --out=release-builds",
    "package-linux-test": "electron-packager . cbox-desktop-admin-app --overwrite --platform=linux --ignore=cred.js --arch=x64 --icon=icons/linux/CBox.png --prune=true --out=release-builds",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "create-debian-installer": "electron-installer-debian --src release-builds/cbox-desktop-admin-app-linux-x64/ --arch amd64 --config debian.json",
    "create-debian-installer-x64": "electron-installer-debian --src release-builds/cbox-desktop-admin-app-linux-x64/ --arch x64 --config debian.json"
  }
}
